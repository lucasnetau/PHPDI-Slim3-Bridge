{
    "name": "edgetelemetrics/php-di-slim3-bridge",
    "description": "PHP-DI integration in Slim 3",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "DI\\Bridge\\Slim\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DI\\Bridge\\Slim\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "php-di/php-di": "^6.4.0 || ^7.0.0",
        "php-di/invoker": "^2.0.0",
        "slim/slim": "^3.12"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5"
    }
}
